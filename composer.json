{
  "name": "GroupEat RESTful API",
  "type": "project",
  "repositories": [
    {
      "type":"vcs",
      "url": "https://github.com/rkgrep/presenter"
    }
  ],
  "require": {
    "anlutro/curl": "0.6.*",
    "tymon/jwt-auth": "0.5.*",
    "pda/pheanstalk": "2.1.*",
    "sebastian/money": "1.5.*",
    "fzaninotto/faker": "1.4.*",
    "laravel/framework": "5.0.*",
    "league/fractal": "0.11.*@dev",
    "Treffynnon/Navigator": "1.0.*",
    "anahkiasen/html-object": "1.4.*",
    "robclancy/presenter": "dev-develop"
  },
  "require-dev": {
    "codeception/codeception": "2.0.*@dev",
    "squizlabs/php_codesniffer": "2.3.*",
    "barryvdh/laravel-ide-helper": "2.0.*"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "Groupeat\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/_support/customs"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
