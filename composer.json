{
  "name": "GroupEat RESTful API",
  "type": "project",
  "require": {
    "tymon/jwt-auth": "0.5.*",
    "pda/pheanstalk": "3.0.*",
    "sebastian/money": "1.5.*",
    "league/fractal": "0.12.*",
    "fzaninotto/faker": "1.4.*",
    "laravel/framework": "5.0.*",
    "guzzlehttp/guzzle": "5.2.*",
    "robclancy/presenter": "1.3.*",
    "Treffynnon/Navigator": "1.0.*",
    "anahkiasen/html-object": "1.4.*"
  },
  "require-dev": {
    "doctrine/dbal": "2.3.*",
    "itsgoingd/clockwork": "1.7.*",
    "squizlabs/php_codesniffer": "2.3.*",
    "codeception/codeception": "2.0.*@dev",
    "barryvdh/laravel-ide-helper": "2.0.*"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "Groupeat\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/_support/customs"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
