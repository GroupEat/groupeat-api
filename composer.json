{
  "name": "GroupEat RESTful API",
  "type": "project",
  "require": {
    "predis/predis": "1.0.*",
    "doctrine/dbal": "2.4.*",
    "league/period": "2.5.*",
    "tymon/jwt-auth": "0.5.*",
    "dingo/api": "1.0.*@dev",
    "sebastian/money": "1.6.*",
    "league/fractal": "0.12.*",
    "fzaninotto/faker": "1.4.*",
    "laravel/framework": "5.2.6",
    "guzzlehttp/guzzle": "5.3.*",
    "robclancy/presenter": "1.3.*",
    "itsgoingd/clockwork": "1.10.*",
    "phaza/laravel-postgis": "2.16",
    "Treffynnon/Navigator": "1.0.*",
    "anahkiasen/html-object": "1.4.*",
    "league/flysystem-aws-s3-v3": "1.0.*"
